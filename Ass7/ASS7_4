         \hline
         1&$0.167$&$0.195$&$0.183$\\
         \hline
         2&$0.167$&$0.195$&$0.183$\\
         \hline
         3&$0.167$&$0.195$&$0.183$\\
         \hline
         4&$0.167$&$0.195$&$0.183$\\
         \hline
         5&$0.167$&$0.216$&$0.26$\\
         \hline
         6&$0.167$&$0.216$&$0.26$\\
         \hline
    \end{tabular}
    \caption{Probability Distribution}
    \label{tab:my_label}
\end{table}
\subsection{Unbiased Die}
\begin{itemize}
    \item The probability of occurrence of each element is same and is  equal to $1/6$.
    \item The theoretical expected value of sum of numbers occurring on upper face of this die is $7$.
    \end{itemize}
\subsection{Biased Die1}
\begin{itemize}
    \item The probability of occurrence of 5 or 6 is $0.216$ while that of other elements is $0.195$
    \item The theoretical expected value of sum of numbers occurring on upper face of this die is $7.6$.
\end{itemize}
\subsection{Biased Die2}
\begin{itemize}
    \item The probability of occurrence of 5 or 6 is $0.26$ while that of other elements is $0.183$
    \item The theoretical expected value of sum of numbers occurring on upper face of this die is  $8.2$ 
\end{itemize}
